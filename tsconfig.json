{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./out",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "declaration": true,
    "lib": ["es2019", "dom"],
    "types": ["node", "axios"],
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts"],
  //"exclude": ["node_modules", "out"]
}